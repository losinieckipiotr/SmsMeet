{
  "extends": "tslint:recommended",
  "rules": {
    "quotemark": [
      true,
      "single"
    ],
    "triple-equals": false,
    "member-access": true,
    "interface-name": [
      true,
      "never-prefix"
    ],
    "max-classes-per-file": false,
    "object-literal-sort-keys": false,
    "no-console": false
  }
}